{
  "name": "@nicksp/prettier-config",
  "description": "Prettier config for @nicksp",
  "version": "1.1.0",
  "type": "module",
  "packageManager": "pnpm@10.26.1",
  "author": {
    "name": "Nick Plekhanov",
    "url": "https://plekhanov.me/"
  },
  "license": "MIT",
  "funding": "https://buymeacoffee.com/plekhanov",
  "homepage": "https://github.com/nicksp/prettier-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicksp/prettier-config.git"
  },
  "bugs": {
    "url": "https://github.com/nicksp/prettier-config/issues"
  },
  "keywords": [
    "prettier",
    "prettier-config"
  ],
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "lint": "eslint --cache",
    "format": "prettier --write \"**/*.{js,ts,md}\"",
    "test": "npx publint && prettier --check examples/",
    "typecheck": "tsgo",
    "release": "bumpp"
  },
  "peerDependencies": {
    "prettier": "^3.7.4"
  },
  "dependencies": {
    "@prettier/plugin-oxc": "^0.1.3"
  },
  "devDependencies": {
    "@nicksp/eslint-config": "^1.5.4",
    "@nicksp/prettier-config": "workspace:*",
    "@types/node": "^25.0.3",
    "@typescript/native-preview": "7.0.0-dev.20251222.1",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4"
  },
  "prettier": "./index.js"
}
